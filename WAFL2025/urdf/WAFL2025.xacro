</joint>



<link name="imu_link">
  <inertial>
    <mass value="0.01" />
    <origin xyz="0 0 0" />
    <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001" />
  </inertial>
</link>

<joint name="imu_joint" type="fixed">
  <parent link="base_link" />  <!-- Attach IMU to the base link -->
  <child link="imu_link" />
  <origin xyz="0 0 0.1" rpy="0 0 0" />  <!-- Adjust position as needed -->
</joint>

<!-- Gazebo IMU plugin -->
<gazebo reference="imu_link">
  <sensor name="imu_sensor" type="imu">
    <always_on>true</always_on>
    <update_rate>100</update_rate>
    <visualize>true</visualize>
    <imu>
      <topic>/imu/data</topic>
      <frame_id>imu_link</frame_id>
      <noise>
        <type>gaussian</type>
        <rate>
          <mean>0.0</mean>
          <stddev>0.0002</stddev>
        </rate>
        <accel>
          <mean>0.0</mean>
          <stddev>0.017</stddev>
        </accel>
      </noise>
    </imu>
  </sensor>
</gazebo>



<!-- Gazebo odometry plugin -->
<gazebo>
  <plugin name="gazebo_ros_odometry" filename="libgazebo_ros_odometry.so">
    <alwaysOn>true</alwaysOn>
    <updateRate>50.0</updateRate>
    <bodyName>base_link</bodyName>  <!-- The link to track for odometry -->
    <topicName>/odom</topicName>    <!-- Topic to publish odometry -->
    <frameName>odom</frameName>     <!-- Reference frame for odometry -->
    <robotNamespace>/</robotNamespace>
  </plugin>
</gazebo>

<gazebo>
